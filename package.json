{
  "name": "hoyo_gacha",
  "version": "1.0.0",
  "description": "An unofficial tool for managing and analyzing your miHoYo gacha records",
  "author": "lgou2w <lgou2w@hotmail.com>",
  "homepage": "https://github.com/lgou2w/HoYo.Gacha#readme",
  "repository": "https://github.com/lgou2w/HoYo.Gacha.git",
  "license": "MIT OR Apache-2.0",
  "private": "true",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=7.0.0"
  },
  "scripts": {
    "preview": "vite preview",
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production tsc && vite build",
    "lint": "eslint --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0 --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,.tsx --ignore-path .gitignore .",
    "test": "pnpm test:web && pnpm test:tauri",
    "test:web": "jest --passWithNoTests --config jest.config.cjs",
    "test:tauri": "cargo test",
    "tauri": "tauri",
    "tauri:dev": "cross-env RUST_LOG=trace RUST_BACKTRACE=full tauri dev",
    "tauri:dev:tools": "cross-env REACT_DEVTOOLS=1 pnpm tauri:dev",
    "tauri:build": "tauri build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.40.0",
    "@fluentui/react-icons": "^2.0.222",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.8.4",
    "@tauri-apps/api": "^1.5.1",
    "i18next": "^23.7.6",
    "immer": "^10.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.19.0",
    "use-immer": "^0.9.0"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.8.4",
    "@tauri-apps/cli": "^1.5.6",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^18.18.9",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-testing-library": "^6.1.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
