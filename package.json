{
  "name": "hoyo_gacha",
  "displayName": "HoYo.Gacha",
  "version": "1.0.0",
  "description": "An unofficial tool for managing and analyzing your miHoYo gacha records",
  "author": "lgou2w <lgou2w@hotmail.com>",
  "homepage": "https://github.com/lgou2w/HoYo.Gacha#readme",
  "repository": "https://github.com/lgou2w/HoYo.Gacha.git",
  "bugs": "https://github.com/lgou2w/HoYo.Gacha/issues",
  "license": "MIT OR Apache-2.0",
  "private": "true",
  "type": "module",
  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
  "engines": {
    "node": "^20.9.0 || >= 22",
    "pnpm": "^10"
  },
  "scripts": {
    "prepare": "husky",
    "clean": "pnpm web:clean && pnpm tauri:clean",
    "dev": "concurrently --raw -k \"pnpm:web:dev\" \"pnpm:backend:dev\"",
    "dev:tools": "cross-env REACT_DEVTOOLS=1 pnpm dev",
    "build": "pnpm web:build && pnpm backend:build",
    "test": "pnpm web:test && pnpm backend:test",
    "lint": "pnpm web:lint && pnpm backend:lint",
    "web:clean": "rimraf dist",
    "web:dev": "cross-env NODE_ENV=development vite",
    "web:build": "cross-env NODE_ENV=production tsc && vite build",
    "web:preview": "cross-env NODE_END=production vite preview --port 1420",
    "web:test": "jest --verbose --passWithNoTests --config jest.config.cjs",
    "web:lint": "eslint --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0 --ignore-path .gitignore .",
    "web:lint:fix": "eslint --fix --ext .ts,.tsx --ignore-path .gitignore .",
    "backend:clean": "cargo clean",
    "backend:dev": "cross-env RUST_LOG=trace RUST_BACKTRACE=full tauri dev",
    "backend:build": "tauri build",
    "backend:test": "cargo test",
    "backend:lint": "cargo check"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.61.2",
    "@fluentui/react-icons": "^2.0.292",
    "@fluentui/react-motion-components-preview": "^0.4.6",
    "@tanstack/react-query": "^5.69.0",
    "@tanstack/react-query-devtools": "^5.69.0",
    "@tanstack/react-router": "^1.114.27",
    "@tauri-apps/api": "^2.4.0",
    "@tauri-apps/plugin-clipboard-manager": "^2.2.2",
    "@tauri-apps/plugin-shell": "^2.2.0",
    "dayjs": "^1.11.13",
    "debounce": "^2.2.0",
    "i18next": "^24.2.3",
    "immer": "^10.1.1",
    "is-promise": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.1",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@griffel/tag-processor": "^1.0.12",
    "@griffel/vite-plugin": "^0.1.12",
    "@tanstack/eslint-plugin-query": "^5.68.0",
    "@tauri-apps/cli": "^2.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.25",
    "@types/react": "^18.3.19",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.9.1",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-n": "^17.16.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-testing-library": "^7.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2",
    "vite": "^6.2.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild"
    ]
  }
}
