{
  "name": "hoyo_gacha",
  "displayName": "HoYo.Gacha",
  "version": "1.1.1",
  "description": "An unofficial tool for managing and analyzing your miHoYo gacha records",
  "author": "lgou2w <lgou2w@hotmail.com>",
  "homepage": "https://github.com/lgou2w/HoYo.Gacha#readme",
  "repository": "https://github.com/lgou2w/HoYo.Gacha",
  "bugs": "https://github.com/lgou2w/HoYo.Gacha/issues",
  "license": "MIT OR Apache-2.0",
  "private": "true",
  "type": "module",
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "engines": {
    "node": "^20.19.0 || >= 22",
    "pnpm": "^10"
  },
  "scripts": {
    "prepare": "husky",
    "clean": "pnpm web:clean && pnpm backend:clean",
    "dev": "concurrently --raw -k \"pnpm:web:dev\" \"pnpm:backend:dev\"",
    "dev:tools": "cross-env REACT_DEVTOOLS=1 pnpm dev",
    "build": "pnpm web:build && pnpm backend:build",
    "build:artifact": "node .scripts/build-artifact.cjs",
    "test": "pnpm web:test && pnpm backend:test",
    "lint": "pnpm web:lint && pnpm backend:lint",
    "web:clean": "rimraf dist",
    "web:dev": "cross-env NODE_ENV=development vite",
    "web:build": "cross-env NODE_ENV=production tsc && vite build",
    "web:preview": "cross-env NODE_END=production vite preview --port 1420",
    "web:test": "jest --verbose --passWithNoTests --config jest.config.cjs",
    "web:lint": "eslint --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0 --ignore-path .gitignore .",
    "web:lint:fix": "eslint --fix --ext .ts,.tsx --ignore-path .gitignore .",
    "backend:clean": "cargo clean",
    "backend:dev": "cross-env RUST_LOG=trace RUST_BACKTRACE=full tauri dev",
    "backend:build": "tauri build",
    "backend:test": "cargo test",
    "backend:lint": "cargo check"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.69.0",
    "@fluentui/react-icons": "^2.0.308",
    "@fluentui/react-motion-components-preview": "^0.9.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@tanstack/react-router": "^1.131.27",
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/plugin-clipboard-manager": "^2.3.0",
    "@tauri-apps/plugin-process": "^2.3.0",
    "@tauri-apps/plugin-shell": "^2.3.1",
    "dayjs": "^1.11.13",
    "debounce": "^2.2.0",
    "i18next": "^25.4.2",
    "immer": "^10.1.1",
    "is-promise": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.7.2",
    "react-virtuoso": "^4.14.0",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@griffel/tag-processor": "^1.0.13",
    "@griffel/vite-plugin": "^0.1.13",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@tauri-apps/cli": "^2.8.3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-testing-library": "^7.6.6",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
